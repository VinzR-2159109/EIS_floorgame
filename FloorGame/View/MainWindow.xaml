<Window x:Class="FloorGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FloorGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Open Debug Window" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="5" Click="OpenDebugWindow" Panel.ZIndex="1" Height="20"/>
        <Frame Name="mainFrame" Margin="0,0,0,0"/>
    </Grid>
</Window>
